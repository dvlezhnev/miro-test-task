{
  "compilerOptions": {
    "target": "ES5",
    "downlevelIteration": true,
    "baseUrl": ".",
    "importHelpers": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitHelpers": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES5",
      "ES6",
      "ES2015",
      "ES2015.Iterable",
      "WebWorker"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
